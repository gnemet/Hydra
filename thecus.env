# --- üê≤ TARGET CONFIGURATION ---
HYDRA_URL=http://192.168.1.250/adm/login.php
HYDRA_TIMEOUT=15
HYDRA_TARGET_SELECTOR=body
HYDRA_SUCCESS_TEXT='"success":true'
HYDRA_ERROR_TEXT='"success":false'

# --- üîë WORDLIST & SEARCH STRATEGY ---
HYDRA_USER_FILE=users.txt
HYDRA_PASS_FILE=passwords.txt
HYDRA_USER_FIELD=p_user,username
HYDRA_PASS_FIELD=p_pass,pwd
HYDRA_EXTRA_PARAMS=eplang=english,action=login,option=com_extplorer

# REGEX: Define base constraints (Smart mode uses these for total lengths)
HYDRA_PASS_REGEX="[a-zA-Z0-9!@#_-]{6,12}"

# BRUTE MODE: Exhaustive sequential
HYDRA_BRUTE_MODE=false

# SMART MODE: Enable human-pattern search (Upper-First, Digit-Last)
# This is much more efficient for real NAS recovery.
HYDRA_SMART_MODE=true

# --- ‚öôÔ∏è PERFORMANCE & SCALE ---
HYDRA_GEN_COUNT=5000000
HYDRA_THREAD_COUNT=4
HYDRA_TEMP_DIR="temp_lists"
